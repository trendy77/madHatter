client
dev tun
proto udp
remote atl-a06.ipvanish.com 443
resolv-retry infinite
nobind
persist-key
persist-tun
persist-remote-ip
ca 'D:\\ipvanish\ca.ipvanish.com.crt'
verify-x509-name atl-a06.ipvanish.com name
auth-user-pass D:\\ipvanish\pass.txt
comp-lzo
verb 3
auth SHA256
cipher AES-256-CBC
keysize 256
tls-cipher TLS-DHE-RSA-WITH-AES-256-CBC-SHA:TLS-DHE-DSS-WITH-AES-256-CBC-SHA:TLS-RSA-WITH-AES-256-CBC-SHA
management localhost 1337